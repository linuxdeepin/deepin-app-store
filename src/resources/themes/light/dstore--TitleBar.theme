
#AppIcon {
  image: url(:/common/images/deepin-app-store.svg);
}

#BackButton {
  qproperty-normalPic: url(:/light/images/backward_normal.svg);
  qproperty-hoverPic: url(:/light/images/backward_hover.svg);
  qproperty-pressPic: url(:/light/images/backward_press.svg);
  qproperty-disabledPic: url(:/light/images/backward_insensitive.svg);
}

#ForwardButton {
  qproperty-normalPic: url(:/light/images/forward_normal.svg);
  qproperty-hoverPic: url(:/light/images/forward_hover.svg);
  qproperty-pressPic: url(:/light/images/forward_press.svg);
  qproperty-disabledPic: url(:/light/images/forward_insensitive.svg);
}

#AvatarButton {
    border-image: url(:/light/images/login_normal.svg);
}
#AvatarButton:hover {
    border-image: url(:/light/images/login_hover.svg);
}
#AvatarButton:pressed {
    border-image: url(:/light/images/login_press.svg);
}

#AvatarButton::menu-indicator {
 image: none;
 width: 0px;
 height: 0px;
}
